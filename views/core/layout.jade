!!! 5
html(ng-app='collabjs')
  head
    title= title
    
    include shared/_meta
    include shared/_styles

    block styles

    script(src='/lib/jquery/js/jquery.min.js')

  body
    input(type='hidden', id='csrf_token', value=collabjs.token)
    include shared/header

    div.container
      div.row
        div.col-md-3.col-lg-3
          block sidebar
            include  shared/sidebar
        div.col-md-9.col-lg-9
          block content

    include shared/_scripts
    block scripts